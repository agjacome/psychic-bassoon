{
  "name": "psychic-bassoon",
  "version": "0.1.0",
  "private": "true",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .js,.ts --color",
    "check:types": "tsc --pretty --noEmit",
    "dev": "NODE_ENV=development nodemon",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "pnpm run check:lint --fix",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "start": "NODE_ENV=production pnpm run build && node ./dist/index.js",
    "test": "NODE_ENV=test DESTROY_SOCKETS=true jest --verbose --colors --detectOpenHandles --runInBand --forceExit",
    "test:e2e": "pnpm run test --testPathPattern=e2e",
    "test:int": "pnpm run test --testPathPattern=integration",
    "test:unit": "pnpm run test --testPathPattern=unit",
    "test:watch": "pnpm run test --watch"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "cors": "^2.8.5",
    "dotenv-defaults": "^5.0.2",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "proquint-": "^1.1.1",
    "superjson": "^1.12.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv-defaults": "^2.0.1",
    "@types/eslint": "^8.40.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/prettier": "^2.7.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "get-tsconfig": "^4.6.0",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "jest-mock-extended": "^3.0.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prettier-plugin-prisma": "^4.13.0",
    "prisma": "^4.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "prisma": {
    "schema": "src/core/infrastructure/prisma/schema.prisma"
  }
}